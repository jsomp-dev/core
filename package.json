{
  "name": "@jsomp/core",
  "version": "1.0.0",
  "description": "JSOMP Core Runtime Container - A stateless, tree-shakable, and injectable JSON-to-Component engine.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "source": "./src/index.ts",
  "scripts": {
    "dev": "tsup src/index.ts --format cjs,esm --watch --dts --splitting",
    "dev:ui": "vite",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "build": "tsup src/index.ts --format cjs,esm --clean --dts --splitting",
    "lint": "tsc --noEmit"
  },
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./plugins": {
      "development": "./src/impl/compiler/plugins/index.ts",
      "types": "./dist/impl/compiler/plugins/index.d.ts",
      "import": "./dist/impl/compiler/plugins/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "jsomp",
    "reactive",
    "renderer",
    "json-to-component",
    "tree-shaking",
    "ioc"
  ],
  "author": "WiseTheWizard",
  "license": "Apache-2.0",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.3.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^5.1.4",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "jsdom": "^28.1.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tailwindcss": "^4.1.18",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "sideEffects": false,
  "dependencies": {
    "zod": "^4.3.6"
  }
}